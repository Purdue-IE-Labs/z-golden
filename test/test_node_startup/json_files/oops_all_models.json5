{
  key: "gedge/examples/tag_updates/updater",
  tags: [
    {
      path: "pump_1/temp",
      type: "model",
      model_path: "mechanical/motor",
      model_version: 1
    },
    {
      path: "joints/position",
      type: "list[model]",
      model_path: "MES/workorder",
      model_version: 0
    },
    {
      path: "my_motor",
      type: "model",
      model_path: "mechanical/motor",
      model_version: 2
    },
    {
      path: "my_motor2",
      type: "model",
      model_path: "mechanical/motor",
      model_version: 2
    },
 
    {
      path: "open_workorders",
      type: "list[model]",
      model_path: "MES/workorder", // If no version is specified, default to latest that is found in model directory
    }
  ],
  methods: [
    {
      path: "my/method/path",
      params: [
          {
              path: "param1",
              type: "model",
              model_path: "mechanical/motor",
              model_version: 3
          },
          {
              path: "param2",
              type: "model",
              model_path: "bar",
              model_version: 100
          }
      ],
      responses: [
          {
              code: 200,
              type: "ok",
              body: [
                  {
                      path: "res1",
                      type: "model",
                      model_path: "bar",
                      model_version: 150
                  }
              ],
              props: {
                  desc: "successfully executed method"
              }
          },
          {
              code: 400,
              type: "err",
              body: [
                  {
                      path: "res1",
                      type: "model",
                      model_path: "bar",
                      model_version: 1000
                  }
              ],
          },
          {
              code: 202,
              type: "info"
          }
      ],
      props: {
          desc: "example method calls"
      },
  }    
  ]
}